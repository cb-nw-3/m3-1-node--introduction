<!DOCTYPE html>
<head>
  <title>Node: Workshop 1</title>
  <link rel="stylesheet" href="./css/styles.css" />
</head>
<body>
  <header>
    <h1>Node: Workshop 1</h1>
  </header>
  <div class="main">
    <h2>Part 1</h2>
    <div class="content">
      <%- include('../partials/questions/5') %>
      <div class="box answer">
        <h3>Answer</h3>
        <!-- render answer here -->
        <h4>Top 5 girls names in the England and Wales</h4>
        <div class="ordered-list" style="top: 5px;">
          <ol>
            <% popularGirlNames.forEach(element => { %>
            <li style="list-style-type: decimal; margin-left: 20px;">
              <%=element%>
            </li>
            <% }); %>
          </ol>
        </div>
      </div>
    </div>
  </div>
</body>
