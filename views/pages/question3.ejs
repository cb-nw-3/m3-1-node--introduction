<%- include('../partials/intro') %>
<body>
  <%- include('../partials/header') %>
  <div class="main">
    <h2>Part 1</h2>
    <div class="content">
      <%- include('../partials/questions/3') %>
      <div class="box answer">
        <h3>Answer</h3>
        <% for (let key in homer) { %> <% if (homer.hasOwnProperty(key)) { %>
        <div>
          <% if (key === "imageUrl") { %>
          <span style="font-weight: bold; font-size: 20px;"
            ><%- key %>:&nbsp;</span
          >
          <span style="font-weight: bold; font-size: 10px;">
            <%- homer[key] %>:&nbsp;</span
          >
          <% } else { %>
          <span style="font-weight: bold; font-size: 20px;"
            ><%- key %>:&nbsp;</span
          >
          <span><%- homer[key] %></span>
          <% } %>
        </div>
        <% } %> <% } %>

        <!-- render answer here -->
      </div>
    </div>
    <a href="/question2" class="previous">&laquo; Previous</a>
    <a
      href="/"
      style="position: absolute; left: 50%; transform: translate(-50%);"
      >Homepage</a
    >
    <a href="/question4" style="float: right;">Next &raquo;</a>
  </div>
</body>
